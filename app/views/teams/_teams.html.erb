<% obj.each do |team|%>
    <tr>
        <td><%=link_to team.name, team_path(team) %></td>
        <td><%=  image_tag team.avatar.url(:thumb), :id => "grb" %></td>
        <% if team.league_id == nil%>
            <td>Unsigned</td>
        <% else %>
            <td><%=link_to team.league.name, :controller => 'leagues', :action => 'show', :id => team.league.id %></td>
        <% end %>
        <td><%= link_to "Statistike tima", :action => 'show', :id => team.id %></td>
    </tr>
<% end %>